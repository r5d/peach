REPO="public.ecr.aws/w3h8c8i8/peach"

# Find the most recent tag.
TAG=$(docker images "$REPO" --format "{{.Tag}}" | head -n 1)
if [ -z "$TAG" ]; then
    echo "No recent tags found"
    exit 1
fi

docker push "$REPO":"$TAG"
